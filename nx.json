{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "test"]
      }
    }
  },
  "targetDefaults": {
    "build": {
      "outputs": ["{projectRoot}/dist"],
      "dependsOn": [
        {
          "target": "build",
          "projects": "dependencies"
        }
      ]
    },
    "test": {
      "dependsOn": [{ "projects": "self", "target": "build" }]
    },
    "lint": {
      "dependsOn": [{ "projects": "self", "target": "build" }]
    }
  },
  "defaultBase": "main"
}

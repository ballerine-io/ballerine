{
  "name": "@ballerine/workflows-service",
  "private": false,
  "version": "0.7.7",
  "description": "workflow-service",
  "scripts": {
    "spellcheck": "cspell \"*\"",
    "setup": "npm run docker:db && npm run db:reset && npm run db:migrate-dev && npm run seed",
    "format": "prettier --write . '!**/*.{md,hbs}'",
    "format:check": "prettier --check . '!**/*.{md,hbs}'",
    "lint": "eslint . --fix",
    "start": "nest start",
    "dev": "npm run start:watch",
    "prod": "npm run db:migrate-up && node dist/src/main",
    "prod:next": "npm run db:migrate-up && npm run db:data-sync && node dist/src/main",
    "start:watch": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "build": "nest build --path=tsconfig.build.json",
    "test": "jest --runInBand",
    "test:unit": "cross-env SKIP_DB_SETUP_TEARDOWN=true jest --testRegex '.*\\.unit\\.test\\.ts$'",
    "test:integration": "jest --testRegex '.*\\.intg\\.test\\.ts$'",
    "test:e2e": "jest --testRegex '.*\\.e2e\\.test\\.ts$'",
    "test:watch": "jest --verbose --watch",
    "seed": "tsx scripts/seed.ts",
    "db:migrate-dev": "prisma migrate dev",
    "db:migrate-up": "prisma migrate deploy",
    "db:clean": "if [ \"$DB_USER\" != \"root\" ]; then tsx scripts/clean.ts; else echo 'Cannot run reset as root'; fi",
    "db:reset": "if [ \"$DB_USER\" != \"root\" ]; then prisma migrate reset --skip-seed -f; else echo 'Cannot run reset as root'; fi",
    "db:reset:dev": "npm run db:reset && npm run seed",
    "db:reset:dev:with-data": "npm run db:reset:dev && npm run db:data-migration:migrate",
    "db:init": "npm run db:migrate-dev -- --name 'initial version' && npm run db:migrate-up seed",
    "prisma:generate": "prisma generate",
    "docker:db": "docker compose -f docker-compose.db.yml up -d --wait",
    "docker:build": "docker build .",
    "compose:up": "docker compose up -d",
    "compose:down": "docker compose down --volumes",
    "upload-sourcemaps": "tsx scripts/upload-sourcemaps.ts",
    "prepare": "npm run prisma:generate",
    "db:data-migration:generate": "plop --plopfile ./prisma/data-migrations/plopfile.js --dest ./",
    "db:data-migration:migrate": "rimraf ./dist && npm run build && nest start --entryFile ./src/data-migration/scripts/migrate",
    "db:data-sync": "nest start --entryFile ./src/data-migration/scripts/sync/run",
    "db:data-migration:import": "tsx ./src/data-migration/scripts/import"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.347.1",
    "@aws-sdk/lib-storage": "3.347.1",
    "@aws-sdk/s3-request-presigner": "3.347.1",
    "@ballerine/common": "0.9.5",
    "@ballerine/workflow-core": "0.6.8",
    "@ballerine/workflow-node-sdk": "0.6.8",
    "@faker-js/faker": "^7.6.0",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^9.3.12",
    "@nestjs/config": "2.3.1",
    "@nestjs/core": "^9.3.12",
    "@nestjs/event-emitter": "^1.4.1",
    "@nestjs/jwt": "10.0.3",
    "@nestjs/passport": "9.0.3",
    "@nestjs/platform-express": "^9.3.12",
    "@nestjs/schedule": "^4.0.1",
    "@nestjs/serve-static": "3.0.1",
    "@nestjs/testing": "^9.3.12",
    "@prisma/client": "4.16.2",
    "@sentry/cli": "^2.17.5",
    "@sentry/integrations": "^7.52.1",
    "@sentry/node": "^7.52.1",
    "@sinclair/typebox": "^0.31.7",
    "@t3-oss/env-core": "^0.6.1",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "ajv-keywords": "^5.1.0",
    "aws-cloudfront-sign": "3.0.2",
    "axios": "^1.6.8",
    "axios-retry": "^4.0.0",
    "base64-stream": "^1.0.0",
    "bcrypt": "5.1.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "concat-stream": "^2.0.0",
    "cookie-session": "^2.0.0",
    "dayjs": "^1.11.6",
    "deep-diff": "^1.0.2",
    "file-type": "^16.5.4",
    "helmet": "^6.0.1",
    "i18n-iso-countries": "^7.6.0",
    "i18next": "^22.4.9",
    "is-what": "^4.1.15",
    "js-base64": "^3.7.6",
    "json-stable-stringify": "^1.1.1",
    "lodash": "^4.17.21",
    "mime": "^3.0.0",
    "multer-s3": "3.0.1",
    "nestjs-cls": "^3.5.0",
    "object-hash": "^3.0.0",
    "passport": "0.6.0",
    "passport-http": "0.3.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "^7.8.0",
    "tmp": "^0.2.1",
    "winston": "^3.9.0",
    "yaml": "^2.3.4",
    "zod": "^3.22.3"
  },
  "devDependencies": {
    "@ballerine/config": "^1.1.2",
    "@ballerine/eslint-config": "^1.1.2",
    "@cspell/cspell-types": "^6.31.1",
    "@nestjs/cli": "9.3.0",
    "@nestjs/swagger": "6.2.1",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/base64-stream": "^1.0.5",
    "@types/bcrypt": "5.0.0",
    "@types/concat-stream": "^2.0.3",
    "@types/cookie-session": "^2.0.44",
    "@types/deep-diff": "^1.0.5",
    "@types/express": "4.17.9",
    "@types/jest": "^26.0.19",
    "@types/jmespath": "^0.15.0",
    "@types/js-base64": "^3.3.1",
    "@types/json-stable-stringify": "^1.0.36",
    "@types/lodash": "^4.14.191",
    "@types/mime": "^3.0.4",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^3.0.0",
    "@types/node": "^18.14.6",
    "@types/object-hash": "^3.0.6",
    "@types/passport": "^1.0.12",
    "@types/passport-http": "0.3.9",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "2.0.11",
    "@types/tmp": "^0.2.3",
    "@typescript-eslint/eslint-plugin": "^5.54.1",

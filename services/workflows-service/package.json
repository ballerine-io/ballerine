{
  "name": "@ballerine/workflows-service",
  "private": false,
  "version": "0.4.9",
  "description": "workflow-service",
  "scripts": {
    "setup": "npm run docker:db && npm run db:clean && npm run db:migrate-save && npm run seed",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint . --fix",
    "start": "nest start",
    "dev": "npm run start:watch",
    "start:watch": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "build": "nest build",
    "test": "jest --verbose",
    "test:watch": "jest --verbose --watch",
    "seed": "ts-node scripts/seed.ts",
    "db:migrate-save": "prisma migrate dev",
    "db:migrate-up": "prisma migrate deploy",
    "db:clean": "ts-node scripts/clean.ts",
    "db:reset": "npm run db:clean && npm run db:migrate-up",
    "db:reset:dev": "npm run db:migrate-up && npm run db:reset && npm run seed",
    "db:init": "npm run db:migrate-save -- --name 'initial version' && npm run db:migrate-up seed",
    "prisma:generate": "prisma generate",
    "docker:db": "docker compose -f docker-compose.db.yml up -d --wait",
    "docker:build": "docker build .",
    "compose:up": "docker compose up -d",
    "compose:down": "docker compose down --volumes",
    "prepare": "npm run prisma:generate"
  },
  "dependencies": {
    "@ballerine/workflow-node-sdk": "^0.4.2",
    "@faker-js/faker": "^7.6.0",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^9.3.12",
    "@nestjs/config": "2.3.1",
    "@nestjs/core": "^9.3.12",
    "@nestjs/devtools-integration": "^0.1.4",
    "@nestjs/event-emitter": "^1.4.1",
    "@nestjs/graphql": "11.0.4",
    "@nestjs/jwt": "10.0.3",
    "@nestjs/passport": "9.0.3",
    "@nestjs/platform-express": "^9.3.12",
    "@nestjs/serve-static": "3.0.1",
    "@nestjs/testing": "^9.3.12",
    "@prisma/client": "^4.13.0",
    "accesscontrol": "^2.2.1",
    "axios": "^1.4.0",
    "bcrypt": "5.1.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "express-session": "^1.17.3",
    "helmet": "^6.0.1",
    "lodash": "^4.17.21",
    "nest-access-control": "2.2.0",
    "nest-morgan": "1.0.1",
    "nestjs-prisma": "0.20.0",
    "passport": "0.6.0",
    "passport-http": "0.3.0",
    "passport-local": "^1.0.0",
    "passport-jwt": "4.0.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "^7.8.0",
    "swagger-ui-express": "4.6.2",
    "ts-node": "9.1.1"
  },
  "devDependencies": {
    "@types/passport": "^1.0.12",
    "@nestjs/cli": "9.3.0",
    "@nestjs/swagger": "6.2.1",
    "@types/bcrypt": "5.0.0",
    "@types/express": "4.17.9",
    "@types/express-session": "^1.17.7",
    "@types/jest": "^26.0.19",
    "@types/lodash": "^4.14.191",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.14.6",
    "@types/normalize-path": "3.0.0",
    "@types/passport-http": "0.3.9",
    "@types/passport-jwt": "3.0.6",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "jest": "27.0.6",
    "jest-mock-extended": "^2.0.4",
    "prettier": "^2.8.4",
    "prisma": "4.13.0",
    "supertest": "4.0.2",
    "ts-jest": "27.0.3",
    "type-fest": "0.11.0",
    "typescript": "4.2.3"
  }
}
